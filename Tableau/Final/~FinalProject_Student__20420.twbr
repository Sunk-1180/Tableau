              <zone forceUpdate='true' h='39250' id='25' type-v2='text' w='98400' x='800' y='59750'>
                <formatted-text>
                  <run fontcolor='#333333' fontsize='16'>KPI Definitions</run>
                  <run>Æ&#10;&#10;</run>
                  <run bold='true' fontcolor='#333333' fontsize='10'>Average Daily Rate</run>
                  <run fontcolor='#333333' fontsize='10'> (ADR): Revenue / Reserved Days</run>
